{
    defaultFaces
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
        //type            mixed;
        refValue        uniform 110000;
        refGradient     uniform 0;
        valueFraction   uniform 0.3;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 110000;

        //type            mixed;
        //refValue        uniform 110000;
        //refGradient     uniform 0;
